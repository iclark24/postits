<h1>Your Notes</h1>
<p><%= link_to 'New Note', new_note_path %></p>

<% @notes.each do |note| %>
<div class="posty">
  <h2><%= note.title %><h2>
   <p><%= note.body%></p>
   <sub><%= note.author%>
   <%= link_to 'Show Note', note_path(note)%>
   <%= link_to 'Edit Note', edit_note_path(note) %>
   <%= link_to 'Del Note', note, method: :delete %>
   </sub>
</div>
<% end %>